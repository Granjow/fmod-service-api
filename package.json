{
  "name": "@geheimgang188/fmod-service-api",
  "version": "1.2.0",
  "description": "FMOD service API",
  "main": "dist/src/index.js",
  "types": "dist/src/index.d.ts",
  "files": [
    "dist/src/*",
    "src/api-generator/templates/*",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "tsc": "tsc",
    "lint": "eslint -c .eslintrc.js \"src/**\" \"spec/**\"",
    "test": "jest",
    "prepack": "rm -rf dist && npm run build",
    "prepublishOnly": "npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:Granjow/fmod-service-api.git"
  },
  "keywords": [
    "fmod",
    "service",
    "zmq",
    "zeromq"
  ],
  "author": "Simon A. Eugster <simon.eu@gmail.com>",
  "license": "ISC",
  "dependencies": {
    "camelcase": "^5.3.1",
    "semaphore-promise": "^1.3.0",
    "small-state-machine": "^1.2.2",
    "zeromq": "^6.0.0-beta.6"
  },
  "devDependencies": {
    "@types/jest": "^27.0.2",
    "@types/node": "^16.11.6",
    "@types/zeromq": "^5.2.1",
    "@typescript-eslint/eslint-plugin": "^4.33.0",
    "@typescript-eslint/parser": "^4.33.0",
    "eslint": "^7.28.0",
    "jest": "^27.3.1",
    "tiny-typed-emitter": "^2.1.0",
    "typescript": "^4.5.5"
  },
  "jest": {
    "clearMocks": true,
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "coverageReporters": [
      "lcov",
      "text"
    ],
    "roots": [
      "dist/"
    ],
    "testEnvironment": "node"
  }
}
